{
  "extends": ["react-app", "react-app/jest", "prettier"],
  "rules": {
    "react/no-unescaped-entities": 0,
    "react-hooks/rules-of-hooks": 0,
    "react-hooks/exhaustive-deps": 0,
    "@next/next/no-img-element": 0,
    "jsx-a11y/alt-text": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "globals": {
    "chrome": "readonly"
  }
}
